{
  "name": "library",
  "version": "0.1.0",
  "description": "library description",
  "main": "lib/library.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "start": "webpack --progress --color -w --env dev",
    "test": "eslint . && istanbul cover _mocha -- --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "istanbul cover _mocha -- --compilers js:babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nelsyeung/es6-lib-template.git"
  },
  "keywords": [
    "browser",
    "library"
  ],
  "author": "Nelson Yeung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nelsyeung/es6-library-template/issues"
  },
  "homepage": "https://github.com/nelsyeung/es6-library-template",
  "dependencies": {
    "core-js": "^2.4.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.2.0",
    "webpack": "^2.2.0",
    "yargs": "^6.5.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "mocha": true
    },
    "extends": "airbnb-base"
  }
}
